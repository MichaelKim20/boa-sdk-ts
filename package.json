{
  "name": "boa-sdk-ts",
  "version": "0.0.36",
  "description": "The TypeScript BOA SDK library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "rimraf ./lib && tsc",
    "test": "rimraf ./lib && tsc && mocha -r ts-node/register tests/**/*.test.ts",
    "testnobuild": "mocha -r ts-node/register tests/**/*.test.ts",
    "build": "rimraf ./lib && tsc",
    "browser": "rimraf ./dist && webpack --config webpack.config.js",
    "docs": "rimraf ./docs && ./node_modules/.bin/typedoc --options ./typedoc.js --module commonjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bosagora/boa-sdk-ts.git"
  },
  "keywords": [
    "SDK",
    "TypeScript"
  ],
  "author": "BOSAGORA Foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bosagora/boa-sdk-ts/issues"
  },
  "homepage": "https://bosagora.github.io/boa-sdk-ts/",
  "dependencies": {
    "@ctrl/ts-base32": "^1.2.1",
    "ajv": "^6.12.6",
    "axios": "^0.21.1",
    "bech32": "^2.0.0",
    "crc": "^3.8.0",
    "jsbi": "^3.1.4",
    "libsodium-wrappers-sumo": "^0.7.9",
    "lodash": "^4.17.19",
    "smart-buffer": "^4.1.0",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@types/bech32": "^1.1.2",
    "@types/chai": "^4.2.11",
    "@types/crc": "^3.4.0",
    "@types/express": "^4.17.6",
    "@types/libsodium-wrappers-sumo": "^0.7.5",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.14",
    "@types/urijs": "^1.19.9",
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
